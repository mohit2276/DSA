class Solution {
    int getPairsCount(int[] arr, int n, int k) {
        // code here,
        int sum=0;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if(arr[i]+arr[j]==k){
                    sum++;
    
                }
                else{
                    continue;
                }
            }
        }
        return sum;
        
    }
}
