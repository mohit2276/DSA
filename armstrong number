import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		int m=sc.nextInt();
		int n=m;
		int sum=0;
		while(n>0){
		    int one=n%10;
		    n=n/10;
		    sum=sum+(one*one*one);
		    
		}
		System.out.println(sum);
		if(m==sum){
		    System.out.println("ARmstrong");
		}
		else{
		    System.out.println("NOT");
		}
		
	}
}
